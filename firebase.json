{
  "hosting": {
    "public": "public",
    "headers": [
      {
        "source": "/.well-known/assetlinks.json",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ]
      }
    ],
    "appAssociation": "AUTO",
    "rewrites": [
      {
        "source": "/link/flow/**",
        "destination": "/index.html"
      }
    ]
  }
}